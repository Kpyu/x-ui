{
  "extends": "airbnb",
  "es6": true,
  "node": true,
  "browser": true,
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "prefer-const": 0,
    "no-var": 0,
    "quote-props": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "global-require": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0,
    "prefer-template": 0,
    "no-console": 0,
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "import/no-unresolved": 0,
    "arrow-body-style": 0,
    "react/sort-comp": 0,
    "react/prop-types": 0,
    "react/jsx-first-prop-new-line": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".md"
        ]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "no-return-assign": 0,
    "max-len": 0,
    "consistent-return": 0,
    "no-redeclare": 0,
    "react/require-extension": 0,
    "react/jsx-indent": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/anchor-has-content": 0,
    "react/no-danger": 0
  }
}